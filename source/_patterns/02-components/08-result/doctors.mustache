{{# doctorsList }}
    <h2 class="doctors-total-result">Total Results: <span class="js-active-results">3</span></h2>
    <ul class="doctor-list">
    {{# doctorItems}}
        <li class="doctor-list__item">
            <div class="doctor-info">
                <div class="doctor-info__general">
                    <div class="doctor-image">
                        <img src="{{{doctorPhoto}}}" alt="" />
                    </div>
                    <div class="doctor-general">
                        <h3 class="doctor-name"><a href="doctorNameUrl">{{doctorName}}</a></h3>
                        <p class="doctor-speciality">Women's Health Dermatology</p>
                    </div>
                </div>
                {{# doctorLocation}}
                <div class="doctor-info__location">
                    <ul class="doctor-location">
                    {{# doctorLocationItem}}
                        <li class="doctor-location__item">
                            <a href="{{{doctorTownUrl}}}" class="doctor-location__link">{{doctorTown}}</a>
                            <div class="doctor-location__distance">{{doctorTownMile}}</div>
                        </li>
                        <li class="doctor-location__item">
                            <a href="{{{doctorFacilityUrl}}}" class="doctor-location__link">{{doctorFacility}}</a>
                            <div class="doctor-location__distance">{{doctorFacilityMile}}</div>
                        </li>
                    {{/ doctorLocationItem}}
                    </ul>
                </div>
                {{/ doctorLocation}}
            </div>
        </li>
    {{/ doctorItems }}
    </ul>
{{/ doctorsList }}